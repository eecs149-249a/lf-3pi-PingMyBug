target C {
    platform: {
        name: "rp2040",
        board: "pololu_3pi_2040_robot"
      },
      threading: false,
}

import LED from "lib/LED.lf";

main reactor ToolsLEDSolution {
    start = new LED();

    timer t1(0 ms, 300 ms);
    timer t2(500 ms, 800 ms);

    reaction(t1) -> start.set {=
        lf_set(start.set, true);

    =}
    reaction(t2) -> start.set {=
        lf_set(start.set, false);
    =}
 
}